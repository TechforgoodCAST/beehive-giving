{
  "search.exclude": {
    "# See https://help.github.com/articles/ignoring-files for more about ignoring files.": true,
    "#": true,
    "# If you find yourself ignoring temporary files generated by your text editor": true,
    "# or operating system, you probably want to add a global ignore instead:": true,
    "#   git config --global core.excludesfile '~/.gitignore_global'": true,
    "# Ignore bundler config.": true,
    "/.bundle": true,
    "# Ignore all logfiles and tempfiles.": true,
    "/log/*": true,
    "/tmp/*": true,
    "!/log/.keep": true,
    "!/tmp/.keep": true,
    "/node_modules": true,
    "/yarn-error.log": true,
    ".byebug_history": true,
    "/public/packs": true,
    "/public/packs-test": true,
    "yarn-debug.log*": true,
    ".yarn-integrity": true,
    "# Misc.": true,
    ".env": true,
    "*.dump": true
  },
