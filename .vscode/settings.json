    "#": true,
    "# If you find yourself ignoring temporary files generated by your text editor": true,
    "# or operating system, you probably want to add a global ignore instead:": true,
    "#   git config --global core.excludesfile '~/.gitignore_global'": true,
    "# Ignore bundler config.": true,
    "/.bundle": true,
    "# Ignore all logfiles and tempfiles.": true,
    "/log/*": true,
    "/tmp/*": true,
